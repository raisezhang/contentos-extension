{
  "name": "contentos-extension",
  "title": "Contentos Wallet",
  "version": "1.0.0",
  "description": "A Contentos Wallet in your Browser",
  "author": "raisezhang@hotmail.com",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "rimraf dist && node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "husky": {
    "hooks": {}
  },
  "dependencies": {
    "@carpenter/vue-tencent-captcha": "^0.2.0",
